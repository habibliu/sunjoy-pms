<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <!-- 这里可以配置 MyBatis 的各种设置 -->
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
        <typeAlias alias="GenTableColumn" type="com.sunjoy.gen.domain.GenTableColumn"/>
        <typeAlias alias="GenTable" type="com.sunjoy.gen.domain.GenTable"/>
    </typeAliases>
</configuration>